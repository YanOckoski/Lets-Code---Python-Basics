    "def soma_lista(lista1, lista2):\n",
    "    nova_lista = []\n",
    "    \n",
    "    for i in range(len(lista1)):\n",
    "        nova_lista.append(lista1[i] + lista2[i])\n",
    "    \n",
    "    return nova_lista\n",
    "\n",
    "lista1 = [1,4,3]\n",
    "lista2 = [3,5,1]\n",
    "\n",
    "soma_lista(lista1, lista2)"
